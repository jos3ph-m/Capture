rd Component\r\nconst Award = ({title,description}) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title}</h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description}</p>\r\n    </AwardStyle>\r\n  )\r\n}\r\n\r\nexport default MovieDetail;","C:\\Users\\joey\\python\\udemy\\dev_ed_react\\Capture\\capture\\src\\movieState.js",[],"C:\\Users\\joey\\python\\udemy\\dev_ed_react\\Capture\\capture\\src\\animation.js",[],"C:\\Users\\joey\\python\\udemy\\dev_ed_react\\Capture\\capture\\src\\components\\Wave.js",[],"C:\\Users\\joey\\python\\udemy\\dev_ed_react\\Capture\\capture\\src\\components\\Toggle.js",["84"],"C:\\Users\\joey\\python\\udemy\\dev_ed_react\\Capture\\capture\\src\\components\\useScroll.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":8,"column":11,"nodeType":"91","endLine":8,"endColumn":33},{"ruleId":"92","severity":1,"message":"93","line":22,"column":10,"nodeType":"94","messageId":"95","endLine":22,"endColumn":18},{"ruleId":"92","severity":1,"message":"96","line":22,"column":20,"nodeType":"94","messageId":"95","endLine":22,"endColumn":29},{"ruleId":"92","severity":1,"message":"97","line":12,"column":18,"nodeType":"94","messageId":"95","endLine":12,"endColumn":27},{"ruleId":"98","severity":1,"message":"99","line":35,"column":9,"nodeType":"91","endLine":35,"endColumn":62},{"ruleId":"92","severity":1,"message":"100","line":3,"column":10,"nodeType":"94","messageId":"95","endLine":3,"endColumn":19},"no-native-reassign",["101"],"no-negated-in-lhs",["102"],"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","no-unused-vars","'elemen